#include<stdio.h>
void main()
{
	int mat1[20][20],mat2[20][20],mat3[20][20];
	int row1,row2,col1,col2,i,j,k;
	printf("Enter the number of rows you want in matrix 1:");
	scanf("%d",&row1);
	printf("Enter the number of columns in matrix 1: ");
	scanf("%d",&col1);
	printf("Enter matrix 1:\n");
	for(i=0;i<row1;i++)
	{
		for(j=0;j<col1;j++)
		{
			scanf("%d",&mat1[i][j]);
		}
	}
	printf("Entered matrix 1 is as follows:\n");
	for(i=0;i<row1;i++)
	{
		for(j=0;j<col1;j++)
		{
			printf("%d\t",mat1[i][j]);
		}
		printf("\n");
	}
	printf("Enter the number of rows you want in matrix 2:");
	scanf("%d",&row2);
	printf("Enter the number of columns in matrix 2: ");
	scanf("%d",&col2);
	printf("Enter matrix 2:\n");
	for(i=0;i<row2;i++)
	{
		for(j=0;j<col2;j++)
		{
			scanf("%d",&mat2[i][j]);
		}
	}
	printf("Entered matrix 2 is as follows:\n");
	for(i=0;i<row2;i++)
	{
		for(j=0;j<col2;j++)
		{
			printf("%d\t",mat2[i][j]);
		}
		printf("\n");
	}
	if(col1!=row2)
	{
		printf("Multiplication not possible");
	}
	else
	{
		for(i=0;i<row1;i++)
		{
			for(j=0;j<col2;j++)
			{
				mat3[i][j]=0;
			    for(k=0;k<col1;k++)
			    {
				     mat3[i][j]+=mat1[i][k]*mat2[k][j];
			    }
	        }
		}
	    printf("Resultant matrix is as follows:\n");
	    for(i=0;i<row1;i++)
	    {
		    for(j=0;j<col2;j++)
		    {
			   printf("%d\t",mat3[i][j]);
		    }
		    printf("\n");
	    }
    }
}
